<h1>Index</h1>

<%= link_to "New article", new_article_path %>

<ul>
  <% @articles.each do |article| %>
  <li><span><%= link_to article.title, article_path(article) %></span> - <%= link_to "Edit", edit_article_path(article) %> - <%= link_to "Delete", article_path(article), method: :delete, data: { confirm: "are your sure you want to delete this article?" } %></li>
  <% end %>
</ul>
